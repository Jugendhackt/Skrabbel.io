CREATE TABLE Insects
(
    InsectID INTEGER NOT NULL, 
    InsectName VARCHAR(20),
    Lebensraum VARCHAR(30),
    Farbe VARCHAR(10),
    Nahrung VARCHAR(15),
    Alter INTEGER
);

